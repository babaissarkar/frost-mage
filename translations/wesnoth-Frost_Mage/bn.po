#: Frost_Mage/scenarios/08_the_temple.cfg:801
msgid "Yes, sir!"
msgstr ""

#. [message]: speaker=unit
#: Frost_Mage/scenarios/08_the_temple.cfg:812
msgid "Yes, my lord!"
msgstr ""

#. [message]: speaker=unit
#: Frost_Mage/scenarios/08_the_temple.cfg:824
msgid "Yes, warchief!"
msgstr ""

#. [message]: speaker=unit
#: Frost_Mage/scenarios/08_the_temple.cfg:833
msgid "Screeee!"
msgstr ""

#. [message]: speaker=lorendor
#: Frost_Mage/scenarios/08_the_temple.cfg:837
msgid "Looks like he found something. We have to go and check it."
msgstr ""

